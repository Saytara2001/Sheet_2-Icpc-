#include <bits/stdc++.h>
using namespace std;
int const N = 5e5 + 5, mod = 1e9 + 7, oo = 1e5 + 10;
typedef long long ll;
int main()
{
    ll n, x;
    ll odd=0,even=0,postive=0,negativ=0;
    cin>>n;
    while(n--)
    {
        cin>>x;
        if(x%2==0)
        {
            even++;
        }
        else if(x%2 != 0)
        {
            odd++;
        }
 
        if(x>0)
        {
            postive++;
        }
        else if(x<0)
        {
            negativ++;
        }
    }
    cout<<"Even: "<<even<<endl;
    cout<<"Odd: "<<odd<<endl;
    cout<<"Positive: "<<postive<<endl;
    cout<<"Negative: "<<negativ<<endl;
    return 0;
}
