#include <bits/stdc++.h>
using namespace std;
int const N = 5e5 + 5, mod = 1e9 + 7, oo = 1e5 + 10;
typedef long long ll;
int main()
{
    ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
    ll n,k;
    cin>>n>>k;
    ll arr[n][n];
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<n;j++)
        {
            if(j==i)
            {
                arr[i][j]=(1*k);
            }else
            {
                arr[i][j]=(0*k);
            }
            cout<<arr[i][j]<<" ";
        }
        cout<<""<<endl;
    }
    return 0;
}
