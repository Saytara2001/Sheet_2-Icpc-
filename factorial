#include <bits/stdc++.h>
using namespace std;
int const N = 5e5 + 5, mod = 1e9 + 7, oo = 1e5 + 10;
typedef long long ll;
int main()
{
    ll n,x,fact=1;
    cin>>n;
    for(int i=1; i<=n;i++)
    {
        cin>>x;
        fact=1;
        for(int j=0;j<x;j++)
        {
            fact*=(x-j);
        }
        cout<<fact<<endl;
    }
    return 0;
}
