#include <bits/stdc++.h>
using namespace std;
int const N = 5e5 + 5, mod = 1e9 + 7, oo = 1e5 + 10;
typedef long long ll;
int main()
{
    ll n,arr[1000];
    cin>>n;
    for(int i=0; i<n; i++)
    {
        cin>>arr[i];
    }
    for(int j=0; j<n; j++)
    {
       if(arr[j]>0)
            cout<<"1"<<" ";
       else if(arr[j]<0)
            cout<<"2"<<" ";
       else
           cout<<"0"<<" ";
    }
    return 0;
}
