#include <bits/stdc++.h>
using namespace std;
int const N = 5e5 + 5, mod = 1e9 + 7, oo = 1e5 + 10;
typedef long long ll;
int main()
{
    ll a,b,GCD=1,check;
    cin>>a>>b;
 
    if(a<b)
        check=a;
    else
        check=b;
 
    for(int i=1; i<=check; i++)
    {
        if((a%i == 0) && (b%i == 0))
             GCD=i;
 
    }
    cout<<GCD<<endl;
    return 0;
}
